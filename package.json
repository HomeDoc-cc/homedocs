{
  "name": "homedocs",
  "version": "0.1.11",
  "private": true,
  "repository": "https://github.com/jhofker/homedocs",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "npx tsx scripts/test-api.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:down": "docker-compose down",
    "docker:db": "docker-compose -f docker-compose.dev.yml up db",
    "docker:app": "docker-compose -f docker-compose.dev.yml up app",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "release": "git push && git tag v$npm_package_version && git push origin v$npm_package_version"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.0",
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/s3-request-presigner": "^3.717.0",
    "@headlessui/react": "^2.2.0",
    "@hookform/resolvers": "^3.9.1",
    "@tanstack/react-query": "^5.62.10",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "ical-generator": "^8.0.1",
    "next": "^15.1.3",
    "next-auth": "^4.24.0",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.54.2",
    "sharp": "^0.33.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.1.0",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
